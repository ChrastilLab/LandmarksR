{
  "metadata": [
    {
      "src": [
        {
          "src": "../Assets/LandmarksR/Scripts",
          "files": [
            "**/*.cs"
          ]
        }
      ],
      "dest": "api",
	  "allowCompilationErrors": true
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "../_site",
    "template": [
	  "default",
	  "modern",
    ],
    "globalMetadata": {
      "_appName": "LandmarksR Documentation",
      "_appTitle": "LandmarksR Documentation",
      "_enableSearch": true,
      "pdf": false
    },
	        "xref": [
            "https://normanderwan.github.io/UnityXrefMaps/xrefmap.yml"
        ],
        "xrefService": [
            "https://xref.docs.microsoft.com/query?uid={uid}"
        ],
  }
}